package nestedLoops;

public class AlphabetLadder {

	public static void main(String[] args) {
		 String letters = "abcdef";
		 
		 for (int a = 0; a < 6; a ++) {
			 for (a; b < 5; b ++) {
				 System.out.println();
			 }
			 System.out.println();
			 
		 }
	}

}
