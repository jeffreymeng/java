package bankAccount;

public class SavingsAccount extends BankAccount {
	double intrest;
	public SavingsAccount(Customer owner, double startingBalance) {
		super(owner, startingBalance);
	}
	
	public SavingsAccount() {
		super();
	}
	
	public payIntrest() {
		balance -= (balance * intrest)
	}
}