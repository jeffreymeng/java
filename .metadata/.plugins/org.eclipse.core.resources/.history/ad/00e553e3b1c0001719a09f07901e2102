package smartbot;

import java.util.Scanner;

public class SmartBot {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		
		System.out.println("SmartBot v0.1.0");
		//System.out.println("Initializing");
		//System.out.println("Initialized");
		System.out.println("Ask any question, type quit to quit, or type help for help.");
		System.out.println("You can even ask me what I can do!");
		
		
		in.close();
		
	}

}
