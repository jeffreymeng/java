package nestedLoops;

public class NestedLoops {

	public static void main(String[] args) {
		for (int a = 0; a < 6; a ++) {
			System.out.print("a");
			for (int b = 0; b < 5; b ++) {
				System.out.print(("b"));
				for (int c = 0; c < 4; c ++) {
					System.out.println("c");
				}
			}
			System.out.println();
		}
	}

}
