package checkingAccountStatic;

public class TestCheckingAccount {
	public static void main(String[] args) {
		System.out.printf("%.2f", CheckingAccount.getBalance());
		CheckingAccount.processDeposit(7.50);
		System.out.printf("%.2f", CheckingAccount.getBalance());
		CheckingAccount.processWithdraw(4.25);
		System.out.printf("%.2f", CheckingAccount.getBalance());
	}
}
