package arraysWorksheet1;

import java.util.Scanner;

public class UserInputArray {

	public static void main(String[] args) {
		int[] age = new int[20];
		for (int i = 0; i < age.legnth; i ++) {
			System.out.println("Enter age #" + i + ":");
		}

	}

}
