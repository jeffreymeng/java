import java.util.Scanner;

public class MutiNumberAdder {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int count;
		
		System.out.print("=======Muti number adder========/nHow many numbers to add?:");
		count = in.nextInt();
		for (int i = 0; i < count; i ++) {
			
		}
		in.close();

	}

}
