package utils;

public class Num {
	public static int getSuffix(int num) {
		String suffix;
		if (num % 100 - num % 10 == 10) {
			suffix = "th";
		} else {
			switch (num % 10) {
			case 1:
				return "st";
			case 2:
				return "nd";
			case 3:
				return "rd";
				break;
			default:
				suffix = "th";
			}
		}
	}
}
