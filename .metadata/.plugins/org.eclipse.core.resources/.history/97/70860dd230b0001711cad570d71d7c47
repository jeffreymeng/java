package nestedLoops;

public class AlphabetLadder {

	public static void main(String[] args) {
		 final String letters = "ABCDEF";
		 
		 for (int a = 1; a < 6; a ++) {
			 for (int b = 0; b < a; b ++) {
				 System.out.print(letters.charAt(b));
			 }
			 System.out.println();
			 
		 }
	}

}
