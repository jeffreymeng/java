package integer;

public class GetSuffix {

	public static void main(String[] args) {
		int num = 11;
		String suffix;
		switch (num % 10){
		case 1:
			suffix = "st";
			break;
		case 2:
			suffix = "nd";
			break;
		case 3:
			suffix = "rd";
			break;
		default:
			suffix = "th";
		}
		
	}

}
