 * Purpose: Print ladder
 */

package nestedLoops;

public class AlphabetLadder {

	public static void main(String[] args) {
		 String letters = "ABCDEF";
		 
		 for (int a = 0; a < letters.length(); a ++) {
			 for (int b = 0; b <= a; b ++) {
				 System.out.print(letters.charAt(b));
			 }
			 System.out.println();
			 
		 }
	}

}
