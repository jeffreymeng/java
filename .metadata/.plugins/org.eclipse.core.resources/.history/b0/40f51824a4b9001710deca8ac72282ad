 */

package methodsWorksheet2;

public class Address {

	public static void main(String[] args) {
		repeatChar('$', 30);

	}
	public static void repeatChar(char charcter, int count) {
		for (int i = 0; i < count; i ++) {
			System.out.print(charcter);
		}
	}

}
