package integer;

public class GetDigit {

	public static void main(String[] args) {
		int num = 283, returnDigit = 2;
		int i, digit = 1;
		for (i = returnDigit - 1; i > 0; i++)
			digit = digit * 10;
		digit = (num / digit) % 10;System.out.println(digit);

	}

}
