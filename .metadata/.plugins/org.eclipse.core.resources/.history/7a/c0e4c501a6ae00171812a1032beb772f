package integer;

public class GetSuffix {

	public static void main(String[] args) {
		
		switch (i % 10){
		case 1:
			suffix = "st";
			break;
		case 2:
			suffix = "nd";
			break;
		case 3:
			suffix = "rd";
			break;
		default:
			suffix = "th";
		}

	}

}
