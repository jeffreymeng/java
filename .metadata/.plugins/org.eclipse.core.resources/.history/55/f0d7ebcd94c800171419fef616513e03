package casino;

import java.util.Scanner;

public class SlotMachine {

	public static void main(String[] args) {
		System.out.println("Type any charcter to spin:");
		Scanner in = new Scanner(System.in)
		in.nextLine();
		System.out.println("You Lose!");
		in.close();
	}

}
